    "start": "hs out",
    "lint": "prettier --check ./**/*.{ts,tsx,md}",
    "format": "prettier --write ./**/*.{ts,tsx,md}",
    "build": "npm run posts && npm run next build && npm run next export",
    "posts": "npm run ts scripts/posts.ts",
    "test": "npm run lint && npm run test-types",
    "test-types": "tsc",
    "deploy": "sh scripts/deploy.sh"
  },
