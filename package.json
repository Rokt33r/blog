    "next": "^9.0.5",
    "nprogress": "^0.2.0",
    "ow": "^0.13.2",
    "ow-my-class": "^0.1.1",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rehype-highlight": "^3.0.0",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1"
